<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Books</title>
</head>
<body>
  <h1>Books</h1>
  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Author</th>
        <th>ISBN</th>
      </tr>
    </thead>
    <tbody>
      <% books.forEach(function(book) { %>
        <tr>
          <td><%= book.book_id %></td>
          <td><%= book.title %></td>
          <td><%= book.author_id %></td>
          <td><%= book.ISBN %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>

</body>
</html>
